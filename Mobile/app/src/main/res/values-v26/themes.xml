<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="viewSplit">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">3dp</item>
        <item name="android:textSize">25sp</item>
        <item name="android:background">@color/main_project_color_one</item>
        <item name="android:layout_marginTop">35dp</item>
        <item name="android:layout_marginBottom">35dp</item>
        <item name="android:layout_marginHorizontal">20dp</item>
    </style>

    <style name="buttonStyleFirst">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginHorizontal">40dp</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@color/loginLableColor</item>
        <item name="android:backgroundTint">@color/main_project_color_two</item>
    </style>
</resources>